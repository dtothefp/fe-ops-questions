{
  "presets": ["react", "es2015-node6/object-rest"],
  "plugins": [
    "add-module-exports",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": false
    }],
    "transform-object-rest-spread",
    "transform-es2015-arrow-functions",
    "syntax-async-functions",
    "bluebird-async-functions",
  ]
}
